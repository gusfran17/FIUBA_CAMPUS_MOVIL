<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fiubapp.fiubapp" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="16"
        android:targetSdkVersion="16" />
    <application
        android:allowBackup="true"
        android:name="com.fiubapp.fiubapp.VolleyController"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register1"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Register2"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".Principal"
            android:theme="@style/StyledIndicators"/>
        <activity android:name=".PerfilTabs"
            android:theme="@style/StyledIndicators"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".PerfilTabsCompanero"
            android:theme="@style/StyledIndicators"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name="ar.uba.fi.fiubappMobile.partners.StudentSearchTabs"
            android:theme="@style/StyledIndicators"
            android:windowSoftInputMode="stateHidden"/>
    </application>

</manifest>
